    End_Procedure
    
    Function IntArrToStrArr Integer[] In Returns String[]
        Integer i
        String[] saOut
        
        For i from 0 to (SizeOfArray(In) - 1)
            Move (String(In[i])) to saOut[SizeOfArray(saOut)]
        Loop
        
        Function_Return saOut
    End_Function
    
